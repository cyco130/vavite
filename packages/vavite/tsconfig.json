{
	"compilerOptions": {
		"noEmit": true,
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"customConditions": ["import"],
		"checkJs": true
	},
	"exclude": [
		"node_modules",
		"dist",
		"cli.js",
		"node-loader.mjs",
		"suppress-loader-warnings.cjs"
	]
}
