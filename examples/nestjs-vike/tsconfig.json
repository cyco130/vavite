{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "es2017",
		"module": "ESNext",
		"esModuleInterop": true,
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"customConditions": ["import"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"jsx": "react-jsx",
		"types": ["vite/client"]
	}
}
